@model TheBookBuffetSem2.Models.Media

@{
    ViewBag.Title = "Delete Media Contact";
}

<h2>Delete Media Contact</h2>

<h3>Are you sure you want to delete this media contact?</h3>
<div>
    <h4>Media</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Organization Name</dt>
        <dd>@Model.Name</dd>

        <dt>Email</dt>
        <dd>@Model.Email</dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <input type="submit" value="Delete" class="btn btn-danger" /> |
@Html.ActionLink("Cancel", "IndexMedia", null, new { @class = "btn btn-secondary" })
    }
</div>
